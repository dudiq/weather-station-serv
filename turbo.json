{
  "tasks": {
    "clean:dist": {
      "cache": false
    },
    "clean:node_modules": {
      "cache": false
    },
    "check-types": {
      "dependsOn": ["build"],
      "outputs": [],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "outputs": []
    }
  }
}
